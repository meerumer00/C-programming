//program to check given character is alphabet, digit or special symbol.
#include<stdio.h>
int main(){
    char ch;
    printf("enter Character :");
    scanf("%C", &ch);
    if((ch>=65 && ch<=90) || (ch>=97 && ch<=122))
    printf("%c is Alphabet\n", ch);
    else if (ch>=48 && ch<=57)
    printf("%c is Digit\n", ch);
    else
    printf("%c is Special Charater\n", ch);
    return 0;
}
