#include<stdio.h>
int main(){
    int j, i, k, n;
    printf("Enter the value of N");
    scanf("%d", &n); 
      for(i=1; i<=n; i++){
        for(j=1; j<=n; j++){
            printf(" ");
        }
        for (k=1; k<=n; k++){
            printf("*");
        }
        printf("\n");        
        }
        return 0;
        }
