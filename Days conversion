#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
   scanf("%d" ,&N);
    int Year,Week,Day;
    Year = N/365;
    N = N%365;
    Week = N/7;
    Day = N%7;
    printf("Years: %d\nWeeks: %d\nDays: %d" ,Year ,Week ,Day);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
