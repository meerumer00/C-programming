#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float P ,R ,T;
    float SI;
    scanf("%f %f %f" ,&P ,&R ,&T);
    SI = (P*R*T)/100;
    printf("%.2f" ,SI);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
